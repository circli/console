{
    "name": "circli/console",
    "description": "wrapper around symfony console to better support containers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Sundqvist",
            "email": "andreas@oak-valley.se"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/console": "^4.0||^5.0||^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "psr/container": "^1.0",
        "phpstan/phpstan": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Circli\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Circli\\Console\\Tests\\": "tests/"
        }
    }
}
